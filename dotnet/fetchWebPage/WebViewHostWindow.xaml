<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ConsoleApp1"
        xmlns:Wpf="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf" x:Class="ConsoleApp1.WebViewHostWindow"
        mc:Ignorable="d"
        Title="WebViewHostWindow" Height="450" Width="800">
    <DockPanel LastChildFill="true">
        <Wpf:WebView2 x:Name="webView" Margin="5" Source="https://moz.com/domain-analysis/google.com" ContentLoading="WebView2_ContentLoading" NavigationCompleted="WebView2_NavigationCompleted" />
    </DockPanel>
</Window>
