{
  "name": "agr-language",
  "version": "0.0.1",
  "private": true,
  "description": "Syntax highlighting for Action Grammar (.agr) files",
  "homepage": "https://github.com/microsoft/TypeAgent#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/TypeAgent.git",
    "directory": "ts/extensions/agr-language"
  },
  "license": "MIT",
  "author": "Microsoft",
  "displayName": "Action Grammar Language",
  "publisher": "typeagent",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "agr",
        "aliases": [
          "Action Grammar",
          "agr"
        ],
        "extensions": [
          ".agr"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "agr",
        "scopeName": "source.agr",
        "path": "./syntaxes/agr.tmLanguage.json"
      }
    ]
  }
}
