// This grammar defines common number patterns (Ordinal and Cardinal)
// These are defined inline and don't require entity declarations

// Ordinal numbers
@ <Ordinal> =
    first -> 1
|   second -> 2
|   third -> 3
|   fourth -> 4
|   fifth -> 5
|   sixth -> 6
|   seventh -> 7
|   eighth -> 8
|   ninth -> 9
|   tenth -> 10
|   eleventh -> 11
|   twelfth -> 12
|   thirteenth -> 13
|   fourteenth -> 14
|   fifteenth -> 15
|   sixteenth -> 16
|   seventeenth -> 17
|   eighteenth -> 18
|   nineteenth -> 19
|   twentieth -> 20
|   twenty\-first -> 21
|   twenty\-second -> 22
|   twenty\-third -> 23
|   twenty\-fourth -> 24
|   twenty\-fifth -> 25
|   twenty\-sixth -> 26
|   twenty\-seventh -> 27
|   twenty\-eighth -> 28
|   twenty\-ninth -> 29
|   thirtieth -> 30

// Cardinal numbers
@ <Cardinal> =
    $(x:number)
|   one -> 1
|   two -> 2
|   three -> 3
|   four -> 4
|   five -> 5
|   six -> 6
|   seven -> 7
|   eight -> 8
|   nine -> 9
|   ten -> 10
|   eleven -> 11
|   twelve -> 12
|   thirteen -> 13
|   fourteen -> 14
|   fifteen -> 15
|   sixteen -> 16
|   seventeen -> 17
|   eighteen -> 18
|   nineteen -> 19
|   twenty -> 20
|   twenty\-one -> 21
|   twenty\-two -> 22
|   twenty\-three -> 23
|   twenty\-four -> 24
|   twenty\-five -> 25
|   twenty\-six -> 26
|   twenty\-seven -> 27
|   twenty\-eight -> 28
|   twenty\-nine -> 29
|   thirty -> 30
