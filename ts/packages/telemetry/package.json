{
  "name": "telemetry",
  "version": "0.0.1",
  "description": "Telemetry provider package for the TypeAgent project.",
  "homepage": "https://github.com/microsoft/TypeAgent#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/TypeAgent.git",
    "directory": "ts/packages/telemetry"
  },
  "license": "MIT",
