  "devDependencies": {
    "@types/debug": "^4.1.10",
    "@types/find-config": "1.0.4",
    "@types/jest": "^29.5.7",
    "@types/node": "^18.18.7",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.2"
