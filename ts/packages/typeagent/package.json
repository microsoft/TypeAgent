    "tsc": "tsc -p src"
  },
  "dependencies": {
    "aiclient": "workspace:*",
    "cheerio": "1.0.0-rc.12",
    "common-utils": "workspace:*",
    "typechat": "^0.1.0",
    "typescript": "^5.4.2"
  },
  "devDependencies": {
    "@types/node": "^18.18.7",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=18"
  }
}
