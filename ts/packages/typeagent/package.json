{
  "name": "typeagent",
  "version": "0.0.10",
  "description": "Typeagent is a library for working with Agents from the Microsoft AI Systems team.",
  "homepage": "https://github.com/microsoft/TypeAgent#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/TypeAgent.git",
    "directory": "ts/packages/typeagent"
  },
  "license": "MIT",
  "author": "Microsoft",
  "exports": {
    ".": "./dist/index.js"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run tsc",
    "clean": "rimraf --glob dist *.tsbuildinfo *.done.build.log",
    "prettier": "prettier --check . --ignore-path ../../.prettierignore",
    "prettier:fix": "prettier --write . --ignore-path ../../prettierignore",
    "test": "node --no-warnings --experimental-vm-modules ./node_modules/jest/bin/jest.js --testPathPattern=\".*\\.spec\\.js\"",
    "test:debug": "node --inspect-brk --no-warnings --experimental-vm-modules ./node_modules/jest/bin/jest.js --testPathPattern=\".*\\.spec\\.js\"",
    "test:full": "node --no-warnings --experimental-vm-modules ./node_modules/jest/bin/jest.js",
    "tsc": "tsc -b"
  },
  "dependencies": {
    "aiclient": "workspace:*",
    "async": "^3.2.5",
    "cheerio": "1.0.0-rc.12",
    "common-utils": "workspace:*",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "typechat": "^0.1.1",
    "typescript": "^5.4.2"
  },
  "devDependencies": {
    "@types/async": "^3.2.24",
    "@types/debug": "^4.1.10",
    "@types/jest": "^29.5.7",
    "@types/node": "^18.18.7",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=18"
  }
}
