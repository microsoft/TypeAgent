{
  "name": "music",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "homepage": "https://github.com/microsoft/TypeAgent#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/TypeAgent.git",
    "directory": "ts/packages/agents/player"
  },
  "license": "MIT",
  "author": "Microsoft",
  "type": "module",
  "exports": {
    "./agent/manifest": "./src/agent/playerManifest.json",
    "./agent/handlers": "./dist/agent/playerHandlers.js"
  },
  "scripts": {
    "asc": "asc -i ./src/agent/playerSchema.ts -o ./dist/agent/playerSchema.pas.json -t PlayerAction",
    "build": "concurrently npm:tsc npm:asc",
    "clean": "rimraf --glob dist *.tsbuildinfo *.done.build.log",
    "tsc": "tsc -p src"
  },
  "dependencies": {
    "@typeagent/agent-sdk": "workspace:*",
    "axios": "^1.8.2",
    "chalk": "^5.4.1",
    "common-utils": "workspace:*",
    "debug": "^4.4.0",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "open": "^7.0.4",
    "typechat": "^0.1.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.17",
    "@types/spotify-api": "^0.0.25",
    "action-schema-compiler": "workspace:*",
    "concurrently": "^9.1.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.4.2"
  },
  "fluidBuild": {
    "declarativeTasks": {
      "asc": {
        "inputGlobs": [
          "src/agent/playerSchema.ts",
          "src/agent/playerSchema.json"
        ],
        "outputGlobs": [
          "dist/agent/playerSchema.json"
        ]
      }
    }
  }
}
