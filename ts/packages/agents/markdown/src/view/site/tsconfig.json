{
  "extends": "../../../../../../tsconfig.base.json",
  "compilerOptions": {
    "noEmit": true,
    "composite": true,
    "incremental": true,
    "rootDir": ".",
    "outDir": "../../../dist/view/site-typecheck",
    "moduleResolution": "bundler",
    "module": "ESNext",
    "lib": ["DOM"],
    "verbatimModuleSyntax": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
  },
  "include": ["./**/*"],
  "ts-node": {
    "esm": true
  }
}
