{
  "name": "plan-visualizer",
  "version": "0.0.1",
  "description": "A dynamic web plan visualizer using Cytoscape.js",
  "homepage": "https://github.com/microsoft/TypeAgent#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/TypeAgent.git",
    "directory": "ts/packages/agents/planVisualizer"
  },
  "license": "MIT",
  "author": "Microsoft",
  "type": "module",
  "exports": {
    "./agent/manifest": "./src/agent/planViewManifest.json",
    "./agent/handlers": "./dist/agent/planViewActionHandler.js"
  },
  "scripts": {
    "build": "npm run build:shared && concurrently npm:build:server npm:build:agent npm:build:client",
    "postbuild": "copyfiles -u 1 \"src/**/config.json\" dist",
    "build:agent": "node scripts/build.mjs agent \"tsc --build src/agent/tsconfig.json\"",
    "build:client": "node scripts/buildFrontend.mjs",
    "build:server": "node scripts/build.mjs server \"tsc --build src/view/server/tsconfig.json\"",
    "build:shared": "node scripts/build.mjs shared \"tsc --build src/view/shared/tsconfig.json\"",
    "clean": "rimraf dist .frontend-build-cache.json .tsbuildinfo && rimraf --glob '.tsc-cache-*.json'",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\" \"npx nodemon --experimental-specifier-resolution=node dist/view/server/server.js 9052 \"",
    "dev:client": "vite dev",
    "dev:server": "tsc --build src/view/server/tsconfig.json --watch",
    "prettier": "prettier --check . --ignore-path ../../../.prettierignore",
    "prettier:fix": "prettier --write . --ignore-path ../../../.prettierignore",
    "start": "node --experimental-specifier-resolution=node dist/view/server/server.js"
  },
  "dependencies": {
    "@typeagent/agent-sdk": "workspace:*",
    "cytoscape": "^3.32.0",
    "cytoscape-dagre": "^2.5.0",
    "dagre": "^0.8.5",
    "debug": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/cytoscape": "^3.21.9",
    "@types/cytoscape-dagre": "^2.3.3",
    "@types/dagre": "^0.7.52",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.10",
    "concurrently": "^9.1.2",
    "copyfiles": "^2.4.1",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.5",
    "vite": "^6.3.4"
  }
}
